{
  "title": "RAG Agent Performance Dashboard",
  "tags": [
    "rag-agent",
    "performance",
    "monitoring"
  ],
  "timezone": "UTC",
  "panels": [
    {
      "title": "Query Performance Overview",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "title": "Query Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "expr": "rate(rag_queries_total[5m])",
          "legendFormat": "Query Rate"
        }
      ]
    },
    {
      "title": "Query Success Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "targets": [
        {
          "expr": "rate(rag_queries_total{status=\"success\"}[5m]) / rate(rag_queries_total[5m]) * 100",
          "legendFormat": "Success Rate %"
        }
      ]
    },
    {
      "title": "Query Latency (95th percentile)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(rag_query_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(rag_query_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile"
        }
      ]
    },
    {
      "title": "Query Error Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 9
      },
      "targets": [
        {
          "expr": "rate(rag_queries_total{status!=\"success\"}[5m])",
          "legendFormat": "Error Rate"
        }
      ]
    },
    {
      "title": "Document Processing Metrics",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 17
      }
    },
    {
      "title": "Documents Processed",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 18
      },
      "targets": [
        {
          "expr": "rate(documents_processed_total[5m])",
          "legendFormat": "{{content_type}} - {{status}}"
        }
      ]
    },
    {
      "title": "Active Documents & Chunks",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 18
      },
      "targets": [
        {
          "expr": "active_documents_total",
          "legendFormat": "Active Documents"
        },
        {
          "expr": "active_chunks_total",
          "legendFormat": "Active Chunks"
        }
      ]
    },
    {
      "title": "Vector Search Performance",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 26
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(vector_search_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(vector_search_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile"
        }
      ]
    },
    {
      "title": "HTTP Request Metrics",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 26
      },
      "targets": [
        {
          "expr": "rate(starlette_requests_total[5m])",
          "legendFormat": "{{method}} {{path}} - {{status_code}}"
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "refresh": "30s",
  "schemaVersion": 27,
  "version": 0,
  "links": []
}